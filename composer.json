{
    "name": "lumesocial/core",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "openai-php/client": "^0.8.0",
        "intervention/image": "^2.7",
        "laravel/sanctum": "*",
        "symfony/http-client": "^7.2",
        "stripe/stripe-php": "^16.5",
        "spatie/laravel-package-tools": "^1.18",
        "inertiajs/inertia-laravel": "^0.6.3",
        "nesbot/carbon": "^2.67",
        "laradumps/laradumps": "^3.5",
        "laravel/horizon": "^5.30",
        "laravel/pail": "^1.2",
        "tightenco/ziggy": "1.4.2",
        "predis/predis": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.0",
        "fakerphp/faker": "^1.9",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "LumeSocial\\": "src/",
            "App\\": "app/",
            "LumeSocial\\Mixpost\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LumeSocial\\Providers\\LumeSocialServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        },
        "platform": {
            "php": "8.2.0"
        }
    }
}
